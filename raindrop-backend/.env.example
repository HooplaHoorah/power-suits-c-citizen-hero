# Example environment variables for Citizen Hero backend
# Copy this file to .env and replace values with your own secrets.
# RAINDROP SmartInference API endpoint and key
RAINDROP_API_URL=https://<raindrop-smartinference-endpoint>
RAINDROP_API_KEY=<your-raindrop-api-key>

# PostgreSQL database URL for quest persistence (e.g., Vultr Managed Postgres)
DATABASE_URL=postgresql://<username>:<password>@<host>:<port>/<database>

# Application environment
APP_ENV=production
